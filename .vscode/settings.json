{
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.mypyEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.pydocstyleEnabled": false,
  "python.testing.pytestArgs": [
    "--import-mode=importlib"
  ],
  "python.envFile": "${workspaceFolder}/.env",
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.rulers": [
      88
    ],
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "cSpell.words": [
    "asdict",
    "astuple",
    "dicttoolz",
    "dtype",
    "EHKID",
    "fspm",
    "FTAS",
    "funcs",
    "functoolz",
    "Gnss",
    "gpkg",
    "hstack",
    "iface",
    "imdecode",
    "imread",
    "imshow",
    "imwrite",
    "inout",
    "insta",
    "interface",
    "itertoolz",
    "krypto",
    "kryptokontext",
    "lxml",
    "noqa",
    "OCISPATIAL",
    "Positionsdatensaetze",
    "pyplot",
    "pyqt",
    "pyqtsignal",
    "qgis",
    "Schluessel",
    "schluesselkryptogramm",
    "Schluesselversion",
    "todos",
    "toolz",
    "vstack",
    "werkzeug"
  ],
  "cSpell.ignoreWords": [
    "EETS",
    "Fsps",
    "GPSDATEN",
    "QOCISPATIAL",
    "applikation",
    "bereich",
    "capgemini",
    "dcmp",
    "dircmp",
    "fahrspurdatei",
    "ftas",
    "gnss",
    "inplace",
    "isoformat",
    "kryptogramm",
    "malkug",
    "ndarray",
    "sequenz",
    "verf",
    "zmax",
    "zmin"
  ],
}
